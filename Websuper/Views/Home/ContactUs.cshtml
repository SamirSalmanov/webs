@model HomeViewModel

<section class="method p-2">
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d6438.73976893638!2d49.83783964668548!3d40.393772885749634!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40307d757b63280d%3A0x1c7f89406ce781e7!2sCompar%20Academy!5e0!3m2!1sru!2s!4v1625482036696!5m2!1sru!2s" width="100%" height="300" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
            </div>
            <div class="col-lg-8">
                <div class="col-lg-12 row">
                    @foreach (var item in Model.ContactUses)
                    {
                        <div class="col-lg-6">
                            <div class="ico">
                                <div class="clock">
                                    <i class="@item.Icon"></i>
                                </div>
                                <div class="text">
                                    <h4>@item.Name</h4>
                                    <p>@item.Text</p>
                                </div>
                            </div>


                        </div>
                    }
                </div>
            </div>


        </div>
    </div>
    </div>

</section>
